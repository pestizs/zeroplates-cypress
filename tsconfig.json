{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"],
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      /* "@fixtures/*":[
        "cypress/fixtures/*"
      ] */
    },
    "isolatedModules": false,
    "allowJs": false,
    //"noEmit": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}